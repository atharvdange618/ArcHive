{
  "expo": {
    "name": "ArcHive",
    "slug": "arc-hive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-arc-hive-final.png",
    "scheme": "archive",
    "deepLinking": true,
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-arc-hive-final.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.atharvdange618.archive"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-arc-hive-final.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-arc-hive-final.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8e0c6da8-f210-4548-8737-c417cd7fb3d7"
      },
      "EXPO_PUBLIC_API_BASE_URL": "https://archive-ctld.onrender.com/api"
    },
    "owner": "atharvdange618"
  }
}
