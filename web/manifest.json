{
  "manifest_version": 3,
  "name": "ArcHIve",
  "version": "1.0",
  "description": "Save current page to ArcHIve",
  "permissions": ["activeTab", "tabs", "storage"],
  "host_permissions": ["https://api.archive.atharvdangedev.in/*"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
